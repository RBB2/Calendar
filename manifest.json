{
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},

	"manifest_version": 2,
	"name": "Calendar",
	"version": "1",
	"author": "Ryan Bartruff",

	"browser_action": {
		"default_icon": "tab-icon.png",
		"default_title": "Calendar"
	},

	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},

	"permissions": ["activeTab"]
}